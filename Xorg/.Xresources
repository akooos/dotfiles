! URXVT
!----------------------------------------------------------------------
URxvt*visualBell:True
! Colores
! bold, italic, underline
URxvt.colorBD: #B5BD68
URxvt.colorIT: #B294BB
URxvt.colorUL: #81A2BE

!https://wiki.archlinux.org/index.php/font_configuration#Presets
URxvt.letterSpace:0
URxvt*font: xft:Hack:size=10,xft:Noto Emoji:bold:size=8:antialias=true:hinting=false,xft:Symbola:size=8:antialias=true:hinting=false
URxvt*boldFont: xft:Hack:style=bold:size=10:autohint=false,xft:Noto Emoji:bold:size=8:antialias=true:hinting=false,xft:Symbola:size=8:antialias=true:hinting=false
URxvt*italicFont: xft:Hack:style=italic:size=10,xft:Noto Emoji:bold:size=8:antialias=true:hinting=false,xft:Symbola:size=8:antialias=true:hinting=false
URxvt*boldItalicFont:  xft:Hack:style=bolditalic:size=10,xft:Noto Emoji:bold:size=8:antialias=true:hinting=false,xft:Symbola:size=8:antialias=true:hinting=false
Xft.size:       8
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintsfull
Xft.autohint:   true
Xft.lcdfilter:  lcddefault
! Varios
URxvt.termName: rxvt-unicode
Rxvt.depth: 32
URxvt*scrollBar: false
URxvt.urgentOnBell: true
URxvt.visualBell: true
URxvt*saveLines: 8044
URxvt*mouseWheelScrollPage: true
! Extensions ~/.urxvt./ext
URxvt.perl-ext-common: default,clipboard,keyboard-select,url-select
! Clipboard
URxvt.keysym.C-S-C: perl:clipboard:copy
URxvt.keysym.C-S-V: perl:clipboard:paste
URxvt.keysym.C-S-W: perl:clipboard:paste_escaped
URxvt.clipboard.copycmd:  xsel -ib
URxvt.clipboard.pastecmd: xsel -ob
! Keyboard-select
URxvt.keysym.C-Escape: perl:keyboard-select:activate
URxvt.keysym.C-S-S: perl:keyboard-select:search
! Url-select
URxvt.keysym.C-S-U: perl:url-select:select_next
URxvt.url-select.autocopy: true
URxvt.url-select.button: 1
URxvt.url-select.launcher: /usr/bin/google-chrome-stable
URxvt.url-select.underline: true
URxvt.background: #000000
URxvt.transparent:   true
URxvt.shading:       20F 
URxvt.foreground: #FFFFFF
URxvt.cursorColor:  #F9EC5E
URxvt.pointerColor: #F9F510

